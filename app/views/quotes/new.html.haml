.container-fluid.add-quote
  .row-fluid
    .featurette
      .span6
        %h2.featurette-heading Make someone's day.
        %h3 No really, say something nice. We all need a boost sometimes
        =form_for @quote do |f|
          =f.label "Message"
          =f.text_area :description
          =f.submit "Submit", class: "btn"
          
      .span6
        = image_tag 'word_cloud.png'
      
      
  